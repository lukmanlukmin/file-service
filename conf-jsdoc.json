{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["./app/"],
    "excludePattern": "node_modules"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "systemName": "api-order-management",
    "footer": "Created by Ralali",
    "copyright": "2016",
    "navType": "vertical",
    "theme": "spacelab",
    "linenums": true,
    "collapseSymbols": true,
    "inverseNav": false
  },
  "opts": {
    "encoding": "utf8",
    "destination": "docs",
    "recurse": true,
    "private": true
  }
}
